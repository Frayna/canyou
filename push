echo "**********************"
echo "**     Git Push     **"
echo "**********************"
echo ''
echo " / Put your Commit \\"
read -p '         ' com
git add *
echo ''
echo ''
echo "**********************"
echo "**     Changes      **"
echo "**********************"
git status
echo ''
echo ''
{
git commit -m $com
git push
} &>/dev/null
